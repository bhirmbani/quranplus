{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "node_modules/.bin/pnpm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "deploy": "node_modules/.bin/pnpm run build && node_modules/.bin/wrangler publish",
    "dev:remix": "remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare -e .env ./build/index.js --watch",
    "dev": "concurrently \"node_modules/.bin/pnpm run dev:css\" \"remix build && run-p \"dev:*\"\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js",
    "test": "jest"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.6.4",
    "@remix-run/cloudflare-workers": "^1.6.4",
    "@remix-run/react": "^1.6.4",
    "axios": "^0.27.2",
    "daisyui": "^2.19.0",
    "dayjs": "^1.11.4",
    "dexie": "^3.2.2",
    "dexie-react-hooks": "^1.1.1",
    "firebase": "^9.9.2",
    "frappe-charts": "^1.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-frappe-charts": "^4.1.0",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@cloudflare/workers-types": "^3.10.0",
    "@remix-run/dev": "^1.6.4",
    "@remix-run/eslint-config": "^1.6.4",
    "@tailwindcss/typography": "^0.5.4",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.7.4",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "autoprefixer": "^10.4.7",
    "babel-jest": "^28.1.3",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.15.0",
    "fake-indexeddb": "^4.0.0",
    "jest": "^28.1.3",
    "miniflare": "^2.4.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^7.5.1",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.6",
    "typescript": "^4.6.4",
    "wrangler": "^2.0.17"
  },
  "engines": {
    "node": ">=14"
  }
}
